.wrapper .submit {
    text-align: right;
    width: 350px;
}

	
	
@media (max-width:750px) {
	#container{
  background-color: #3C873C;
       border: 1px solid #3C873C;
	}
    .ProductStockEntry {
         width: 95%;
		 position:static;
    }
    .wrapper label {
        width:100%;
        display: block;
    }
    .wrapper textarea {
        width:80%;
		height:10%;
		color: red;
    }
    .wrapper .submit {
        width: 80%;
    }
	.wrapper .required {
    position: static;
 
}
#ui-datepicker-div{
width: 80%;
 background-color:lightgreen;
 font-size: 60%;
}
}
@media (min-width:750px) {
#ui-datepicker-div{
width:  200px;
 background-color:lightgreen;
 font-size: 14px;
}
}
.ui-autocomplete { max-height: 200px; overflow-y: auto; overflow-x: hidden;width: 100px; border: 4px solid #aaa; }  

.ui-datepicker {
   background: #333;
   border: 1px solid #555;
   color: #EEE;
 }
